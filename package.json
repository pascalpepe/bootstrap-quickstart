{
  "name": "bootstrap-project-template",
  "version": "1.0.0",
  "private": true,
  "description": "Project template for a quick start with Bootstrap framework.",
  "keywords": [
    "web",
    "bootstrap",
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end"
  ],
  "homepage": "https://gitlab.com/pascalpepe/bootstrap-project-template#readme",
  "author": "Pascal Pepe",
  "scripts": {
    "css-compile": "npm-run-all --parallel css-compile-*",
    "css-compile-main": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/main.scss dist/css/main.css",
    "css-lint": "npm-run-all --parallel css-lint-*",
    "css-lint-main": "stylelint \"scss/**/*.scss\" --cache --cache-location .cache/.stylelintcache",
    "css-minify": "npm-run-all --parallel css-minify-*",
    "css-minify-main": "cleancss --level 1 --format breaksWith=lf --source-map --source-map-inline-sources --output dist/css/main.min.css dist/css/main.css"
  },
  "style": "dist/css/theme.css",
  "sass": "scss/theme.scss",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/pascalpepe/bootstrap-project-template.git"
  },
  "bugs": {
    "url": "https://gitlab.com/pascalpepe/bootstrap-project-template/issues"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "bootstrap": "^4.2.1",
    "clean-css-cli": "^4.2.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "stylelint": "^9.10.0",
    "stylelint-config-twbs-bootstrap": "^0.2.1"
  },
  "peerDependencies": {
    "jquery": "1.9.1 - 3",
    "popper.js": "^1.14.6"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist/css/*.{css,map}",
    "scss/**/*.scss"
  ]
}
