{
  "name": "bootstrap-quickstart",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "css": "npm-run-all css-lint css-compile css-prefix css-minify",
    "css-compile": "node-sass --output-style expanded --include-path node_modules/ --source-map true --source-map-contents true --precision 6 scss/main.scss dist/css/main.css",
    "css-lint": "stylelint \"scss/**/*.scss\" --cache --cache-location .cache/.stylelintcache",
    "css-minify": "cleancss --level 1 --format breaksWith=lf --source-map --source-map-inline-sources --output dist/css/main.min.css dist/css/main.css",
    "css-prefix": "postcss --config postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css-watch": "nodemon --watch scss/ --ext scss --exec \"npm run css\""
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "clean-css-cli": "^4.3.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "stylelint": "^10.1.0",
    "stylelint-config-twbs-bootstrap": "^0.4.0"
  },
  "engines": {
    "node": ">=6"
  }
}
